   "source": [
    "def getWeather(icao_code):\n",
    "    ret = requests.get('https://aw.stephenorjames.com/api/retrieve/report/%s' % icao_code)\n",
    "    data = json.loads(ret.text)\n",
    "    return data"
   ]
  },
